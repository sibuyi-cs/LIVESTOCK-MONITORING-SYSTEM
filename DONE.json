    {
        "id": "78e0a7433dbf3bf9",
        "type": "function",
        "z": "50bfe2492e483213",
        "name": "Send data to DataBase",
        "func": "\n\nvar humidityValue = msg.payload['uplink_message']['decoded_payload']['relative_humidity_2']\nvar temperatureValue = msg.payload['uplink_message']['decoded_payload']['temperature_1']\nvar speed = msg.payload['uplink_message']['decoded_payload']['analog_in_1']\nvar light_intensity = msg.payload['uplink_message']['decoded_payload']['analog_in_2']\n\n\nvar latitude = msg.payload['uplink_message']['decoded_payload']['gps_1']['latitude']\nvar longitude = msg.payload['uplink_message']['decoded_payload']['gps_1']['longitude']\nvar altitude = msg.payload['uplink_message']['decoded_payload']['gps_1']['altitude']\n\n\nvar deviceNameValue = 'TTGO Lora32';\n\nmsg.payload = [deviceNameValue, temperatureValue, humidityValue, latitude, longitude, speed, altitude, light_intensity];\nmsg.topic = \"INSERT INTO liveStockDB  (deviceName,temperature,humidity,latitude,longitude,speed,altitude,lightIntensity) VALUES (?,?,?,?,?,?,?,?)\";\nreturn msg;\n\n\n",
